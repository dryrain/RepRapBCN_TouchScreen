015592  000679      i := -1 ;
015593  000680      j := *(typeptr) ;
015594  000681      repeat
015595  000682          typeptr += 2 ;
015596  000683          i++ ;
015597  000684          until (*(typeptr) == imgidx);
015598  000685      j := setptr + i*10 ;
015599  000686        
015600  000687      imgidx++ ;
015601  000688      l := 1 ;
015602  000689      for (i := 1; i < *(j+Ofs_Digits_Digits); i++)
015603  000690          l *= 10 ;
