012048  000549  func ledDigitsDisplay(var imgidx, var typeptr, var setptr)
012049  000550      var i, j, k, l, lb, newval ;
012050  000551      if (!((img_GetWord(hndl, imgidx, IMAGE_FLAGS) & I_ENABLED))) return ;  // ;img_GetWord(hndl, imgidx, IMAGE_TAG2) ;if diabled then exit
012051  000552      newval := img_GetWord(hndl, imgidx, IMAGE_TAG2) ;
012052  000553      i := -1 ;
012053  000554      j := *(typeptr) ;
012054  000555      repeat
012055  000556          typeptr += 2 ;
012056  000557          i++ ;
012057  000558          until (*(typeptr) == imgidx);
012058  000559      j := setptr + i*10 ;
012059  000560        
012060  000561      imgidx++ ;
012061  000562      l := 1 ;
012062  000563      for (i := 1; i < *(j+Ofs_Digits_Digits); i++)
012063  000564          l *= 10 ;
012064  000565      next
012065  000566      lb := *(j+Ofs_Digits_LeadingBlanks) ;
012066  000567      for (i := 0; i < *(j+Ofs_Digits_Digits); i++)
012067  000568          k := newval / l ;
012068  000569          newval -= k * l ;
012069  000570          if ( lb && (i < *(j+Ofs_Digits_Digits) - *(j+Ofs_Digits_MinDigits)) )
012070  000571              if (k == 0)
012071  000572                  k := 10 ;
012072  000573              else
012073  000574                  lb := 0 ;
012074  000575              endif
012075  000576          endif
012076  000577          l /= 10 ;
012077  000578          img_SetWord(hndl, imgidx, IMAGE_INDEX, k);
012078  000579          img_SetWord(hndl, imgidx, IMAGE_XPOS, *(j+Ofs_Digits_Left)+i* *(j+Ofs_Digits_Widthdigit)) ;
012079  000580          img_Show(hndl, imgidx);
012080  000581      next
012081  000582  endfunc
