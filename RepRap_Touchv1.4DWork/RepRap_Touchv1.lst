015531  000618          switch (ObjectType)
015532  000619              case tWinButton :
015533  000620                  j := InputControls[oWinButtons[ObjectIdx+1]] ;
015534  000621                  break ;
015535  000622              case tUserButton :
015536  000623                  j := InputControls[oUserButtons[ObjectIdx+1]] ;
015537  000624                  break ;
015538  000625              default : j := -1 ;
015539  000626          endswitch
015540  000627          if (j != -1)
015541  000628              k := img_GetWord(hndl, i , IMAGE_INDEX) ;
015542  000629              NewVal := NewVal << 1 ;
015543  000630              if (OVF())      // button group index change
015544  000631                  if (*(j+InputData+Ofs_IPD_P1))
015545  000632                      k &= 1 ;    // mask off old group index for momentary
