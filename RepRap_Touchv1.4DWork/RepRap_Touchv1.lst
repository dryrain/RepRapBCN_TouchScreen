012128  000629  func UpdateObjects(var newval)
012129  000630      var IPidx, otherOBJ ;
012130  000631      if ( ( img_GetWord(hndl, *(pInputIndex), IMAGE_INDEX) != newval) || (TouchState == Ofs_IPD_RELEASE) ) // only bother if values changed, or release
012131  000632        
012132  000633          img_SetWord(hndl, *(pInputIndex), IMAGE_INDEX, newval);
012133  000634              img_Show(hndl, *(pInputIndex));      // only shows on current form
012134  000635          if ((InputType == t4Dbutton) || (InputType == tUserButton) || (InputType == tWinButton))
012135  000636              if (*(CurInputData+Ofs_IPD_P1))
012136  000637                  newval &= 1;
012137  000638              else
012138  000639                  newval &= 3;
012139  000640              endif
012140  000641              if (newval > 1) newval := 1;
012141  000642          endif
012142  000643          IPidx := *(CurInputData+TouchState) ;
012143  000644          while(IPidx != 0)
012144  000645              otherOBJ := IPidx + InputData;
012145  000646              if (*(otherOBJ) == OT_REPORT)
012146  000647          SendReport(REPORT_EVENT, InputType, *(otherOBJ+Ofs_IPD_OBJVIDX), newval) ;
012147  000648              else if (*(otherOBJ) == OT_MAGIC)
012148  000649                  IPidx := *(otherOBJ+Ofs_IPD_P5) ;
012149  000650                  IPidx(newval) ;
012150  000651              else if (TouchState == *(otherOBJ+Ofs_IPD_P4))
012151  000652                  if (*(otherOBJ) == OT_ACTIVATE)
012152  000653                      ActivateForm(*(otherOBJ+Ofs_IPD_P2) ) ;
012153  000654                      InputType := tForm ;
012154  000655                  else if (*(otherOBJ) == OT_SETCONST)
012155  000656                      newval := *(otherOBJ+Ofs_IPD_P3) ;
012156  000657                      WriteObject(*(otherOBJ+Ofs_IPD_P1), *(otherOBJ+Ofs_IPD_P2), newval) ;
012157  000658                  else if (*(otherOBJ) == OT_SETANOTHER)
012158  000659                      WriteObject(*(otherOBJ+Ofs_IPD_P1), *(otherOBJ+Ofs_IPD_P2), newval) ;
012159  000660                  else if (*(otherOBJ) == OT_PREVFRAME)
012160  000661                      if (img_GetWord(hndl, *(otherOBJ+Ofs_IPD_P6), IMAGE_INDEX))
012161  000662                          WriteObject(*(otherOBJ+Ofs_IPD_P5),*(otherOBJ+Ofs_IPD_P2),img_GetWord(hndl, *(otherOBJ+Ofs_IPD_P6), IMAGE_INDEX)-1) ;
012162  000663                      endif
012163  000664                      newval := img_GetWord(hndl, *(otherOBJ+Ofs_IPD_P6), IMAGE_INDEX) ;
012164  000665                  else if (*(otherOBJ) == OT_NEXTFRAME)
012165  000666                      if (img_GetWord(hndl, *(otherOBJ+Ofs_IPD_P6), IMAGE_INDEX) < *(otherOBJ+Ofs_IPD_P3))
012166  000667                          WriteObject(*(otherOBJ+Ofs_IPD_P5),*(otherOBJ+Ofs_IPD_P2),img_GetWord(hndl, *(otherOBJ+Ofs_IPD_P6), IMAGE_INDEX)+1) ;
012167  000668                      endif
012168  000669                      newval := img_GetWord(hndl, *(otherOBJ+Ofs_IPD_P6), IMAGE_INDEX) ;
012169  000670                  else if (*(otherOBJ) == OT_PREVSTRING)
012170  000671                      if (stringsCV[*(otherOBJ+Ofs_IPD_P2)])
012171  000672                          WriteObject(tStrings,*(otherOBJ+Ofs_IPD_P2),stringsCV[*(otherOBJ+Ofs_IPD_P2)]-1) ;
012172  000673                      endif
012173  000674                  else if (*(otherOBJ) == OT_NEXTSTRING)
012174  000675                      if (stringsCV[*(otherOBJ+Ofs_IPD_P2)] < *(otherOBJ+Ofs_IPD_P3)) // fix IPD_P2 not filled in yet
012175  000676                          WriteObject(tStrings,*(otherOBJ+Ofs_IPD_P2),stringsCV[*(otherOBJ+Ofs_IPD_P2)]+1) ;
012176  000677                      endif
012177  000678                  endif
012178  000679              endif
012179  000680              IPidx := *(otherOBJ+TouchState) ;
012180  000681          wend
012181  000682      endif
012182  000683  endfunc
