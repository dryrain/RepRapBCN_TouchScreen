015588  000675  func ledDigitsDisplay(var imgidx, var typeptr, var setptr)
015589  000676      var i, j, k, l, lb, newval ;
015590  000677      if (!((img_GetWord(hndl, imgidx, IMAGE_FLAGS) & I_ENABLED))) return ;  // ;img_GetWord(hndl, imgidx, IMAGE_TAG2) ;if diabled then exit
