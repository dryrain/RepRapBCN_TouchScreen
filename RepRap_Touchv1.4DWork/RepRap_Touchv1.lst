012207  000603          gfx_Set(BACKGROUND_COLOUR,FormBGcolors[CurrentForm]);
012208  000604          gfx_Cls() ;
012209  000605          DoGFXObjects() ;                                    // display GFX 'widgets'
012210  000606      endif
012211  000607        
