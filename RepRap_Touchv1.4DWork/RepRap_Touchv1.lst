012217  000613              img_SetAttributes(hndl, i, I_STAYONTOP+I_ENABLED);        // make sure this is on top of form, if applicable
012218  000614              //if (j != tKeyboard)
012219  000615              if ((j <= tWinButton) || (j >= t4Dbutton) )               // enable inputs
012220  000616                  img_ClearAttributes(hndl, i, I_TOUCH_DISABLE);        // ensure touch is enabled
