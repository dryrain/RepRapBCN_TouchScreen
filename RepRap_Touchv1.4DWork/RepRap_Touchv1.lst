012238  000634  func UpdateObjects(var newval)
012239  000635      var IPidx, otherOBJ ;
012240  000636      if ( ( img_GetWord(hndl, *(pInputIndex), IMAGE_INDEX) != newval) || (TouchState == Ofs_IPD_RELEASE) ) // only bother if values changed, or release
012241  000637        
012242  000638          img_SetWord(hndl, *(pInputIndex), IMAGE_INDEX, newval);
012243  000639              img_Show(hndl, *(pInputIndex));      // only shows on current form
012244  000640          if ((InputType == t4Dbutton) || (InputType == tUserButton) || (InputType == tWinButton))
012245  000641              if (*(CurInputData+Ofs_IPD_P1))
012246  000642                  newval &= 1;
012247  000643              else
012248  000644                  newval &= 3;
012249  000645              endif
