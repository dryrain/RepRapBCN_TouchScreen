012194  000590  func ActivateForm(var newform)
012195  000591      var i, j, *p ;
012196  000592        
012197  000593      if (CurrentForm != -1) // deactivate old form, by disabling all inputs
012198  000594          for (i := FormStartIndex[CurrentForm]; i <= FormEndIndex[CurrentForm]; i++)
012199  000595              if (img_GetWord(hndl, i, IMAGE_TAG))
012200  000596                  img_Disable(hndl,i) ;
