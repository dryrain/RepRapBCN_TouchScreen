012158  000554  func ledDigitsDisplay(var imgidx, var typeptr, var setptr)
012159  000555      var i, j, k, l, lb, newval ;
012160  000556      if (!((img_GetWord(hndl, imgidx, IMAGE_FLAGS) & I_ENABLED))) return ;  // ;img_GetWord(hndl, imgidx, IMAGE_TAG2) ;if diabled then exit
012161  000557      newval := img_GetWord(hndl, imgidx, IMAGE_TAG2) ;
012162  000558      i := -1 ;
012163  000559      j := *(typeptr) ;
012164  000560      repeat
012165  000561          typeptr += 2 ;
012166  000562          i++ ;
012167  000563          until (*(typeptr) == imgidx);
012168  000564      j := setptr + i*10 ;
012169  000565        
012170  000566      imgidx++ ;
012171  000567      l := 1 ;
012172  000568      for (i := 1; i < *(j+Ofs_Digits_Digits); i++)
012173  000569          l *= 10 ;
012174  000570      next
012175  000571      lb := *(j+Ofs_Digits_LeadingBlanks) ;
012176  000572      for (i := 0; i < *(j+Ofs_Digits_Digits); i++)
012177  000573          k := newval / l ;
012178  000574          newval -= k * l ;
012179  000575          if ( lb && (i < *(j+Ofs_Digits_Digits) - *(j+Ofs_Digits_MinDigits)) )
012180  000576              if (k == 0)
012181  000577                  k := 10 ;
012182  000578              else
012183  000579                  lb := 0 ;
012184  000580              endif
012185  000581          endif
012186  000582          l /= 10 ;
012187  000583          img_SetWord(hndl, imgidx, IMAGE_INDEX, k);
012188  000584          img_SetWord(hndl, imgidx, IMAGE_XPOS, *(j+Ofs_Digits_Left)+i* *(j+Ofs_Digits_Widthdigit)) ;
012189  000585          img_Show(hndl, imgidx);
012190  000586      next
012191  000587  endfunc
