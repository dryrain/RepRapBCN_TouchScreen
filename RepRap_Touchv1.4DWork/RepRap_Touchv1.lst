015548  000635                  endif
015549  000636              else            // normal set
015550  000637                  if (*(j+InputData+Ofs_IPD_P2) != -1) TurnOffButtons(*(j+InputData+Ofs_IPD_P2)) ;
015551  000638                  k &= 0xfffc ;    // retain only group index for state set
015552  000639              endif
015553  000640              NewVal |= k ;
015554  000641          endif
015555  000642           if (ObjectType == tLeddigits)
015556  000643              img_SetWord(hndl, i , IMAGE_TAG2, NewVal); // where state is 0 to 2
