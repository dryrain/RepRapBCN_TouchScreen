012277  000673                      endif
012278  000674                      newval := img_GetWord(hndl, *(otherOBJ+Ofs_IPD_P6), IMAGE_INDEX) ;
012279  000675                  else if (*(otherOBJ) == OT_PREVSTRING)
012280  000676                      if (stringsCV[*(otherOBJ+Ofs_IPD_P2)])
012281  000677                          WriteObject(tStrings,*(otherOBJ+Ofs_IPD_P2),stringsCV[*(otherOBJ+Ofs_IPD_P2)]-1) ;
012282  000678                      endif
012283  000679                  else if (*(otherOBJ) == OT_NEXTSTRING)
012284  000680                      if (stringsCV[*(otherOBJ+Ofs_IPD_P2)] < *(otherOBJ+Ofs_IPD_P3)) // fix IPD_P2 not filled in yet
012285  000681                          WriteObject(tStrings,*(otherOBJ+Ofs_IPD_P2),stringsCV[*(otherOBJ+Ofs_IPD_P2)]+1) ;
012286  000682                      endif
012287  000683                  endif
012288  000684              endif
012289  000685              IPidx := *(otherOBJ+TouchState) ;
012290  000686          wend
012291  000687      endif
