015612  000699              else
015613  000700                  lb := 0 ;
015614  000701              endif
015615  000702          endif
015616  000703          l /= 10 ;
015617  000704          img_SetWord(hndl, imgidx, IMAGE_INDEX, k);
015618  000705          img_SetWord(hndl, imgidx, IMAGE_XPOS, *(j+Ofs_Digits_Left)+i* *(j+Ofs_Digits_Widthdigit)) ;
