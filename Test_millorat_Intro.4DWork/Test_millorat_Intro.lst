006137  000329  func WriteObject(var ObjectType, var ObjectIdx, var NewVal)
006138  000330      var i, j, k, Objects ;
006139  000331      ObjectType &= 0x3f ;
006140  000332      if (ObjectType == tForm)
006141  000333          ActivateForm(ObjectIdx) ;
006142  000334      else
006143  000335          Objects := *(oObjects+ObjectType)+ObjectIdx*2+2 ;
006144  000336          i := *(Objects) ;
006145  000337          switch (ObjectType)
006146  000338              case tWinButton :
006147  000339                  j := IPControls[oWinButtons[ObjectIdx+1]] ;
006148  000340                  break ;
006149  000341              case tUserButton :
006150  000342                  j := IPControls[oUserButtons[ObjectIdx+1]] ;
006151  000343                  break ;
006152  000344              default : j := -1 ;
006153  000345          endswitch
006154  000346          if (j != -1) 
006155  000347              k := img_GetWord(hndl, i , IMAGE_INDEX) ;
006156  000348              NewVal := NewVal << 1 ;  
006157  000349              if (OVF())      // button group index change
006158  000350                  if (*(j+IPData+IPD_P1))   
006159  000351                      k &= 1 ;    // mask off old group index for momentary
006160  000352                  else
006161  000353                      k &= 3 ;    // mask off old group index for toggle
006162  000354                  endif
006163  000355              else            // normal set
006164  000356                  if (*(j+IPData+IPD_P2) != -1) TurnOffButtons(*(j+IPData+IPD_P2)) ;
006165  000357                  k &= 0xfffc ;    // retain only group index for state set
006166  000358              endif
006167  000359              NewVal |= k ;
006168  000360          endif
006169  000361           if (ObjectType == tLeddigits)
006170  000362              img_SetWord(hndl, i , IMAGE_TAG2, NewVal); // where state is 0 to 2
006171  000363              ledDigitsDisplay(i, oLeddigitss, OPDataL) ;
006172  000364          else if (ObjectType == tStrings)
006173  000365              PrintStrings(ObjectIdx, NewVal, 0);
006174  000366          else
006175  000367              img_SetWord(hndl, i , IMAGE_INDEX, NewVal); // where state is 0 to 2
006176  000368              img_Show(hndl, i) ; // will only display if form is current
006177  000369          endif
006178  000370      endif
006179  000371  endfunc
