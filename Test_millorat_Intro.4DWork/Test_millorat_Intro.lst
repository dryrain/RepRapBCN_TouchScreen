008362  000475  func ledDigitsDisplay(var imgidx, var typeptr, var setptr)
008363  000476      var i, j, k, l, lb, newval ;
008364  000477      if (!((img_GetWord(hndl, imgidx, IMAGE_FLAGS) & I_ENABLED))) return ;  // ;img_GetWord(hndl, imgidx, IMAGE_TAG2) ;if diabled then exit
008365  000478      newval := img_GetWord(hndl, imgidx, IMAGE_TAG2) ;
008366  000479      i := -1 ;
008367  000480      j := *(typeptr) ;
008368  000481      repeat
008369  000482          typeptr += 2 ;
008370  000483          i++ ;
008371  000484          until (*(typeptr) == imgidx);
008372  000485      j := setptr + i*10 ;
008373  000486        
008374  000487      imgidx++ ;
008375  000488      l := 1 ;
008376  000489      for (i := 1; i < *(j+iDigits); i++)
008377  000490          l *= 10 ;
008378  000491      next
008379  000492      lb := *(j+iLeadingBlanks) ;
008380  000493      for (i := 0; i < *(j+iDigits); i++)
008381  000494          k := newval / l ;
008382  000495          newval -= k * l ;
008383  000496          if ( lb && (i < *(j+iDigits) - *(j+iMinDigits)) )
008384  000497              if (k == 0)
008385  000498                  k := 10 ;
008386  000499              else
008387  000500                  lb := 0 ;
008388  000501              endif
008389  000502          endif
008390  000503          l /= 10 ;
008391  000504          img_SetWord(hndl, imgidx, IMAGE_INDEX, k);
008392  000505          img_SetWord(hndl, imgidx, IMAGE_XPOS, *(j+ileft)+i* *(j+iWidthDigit)) ;
008393  000506          img_Show(hndl, imgidx);
