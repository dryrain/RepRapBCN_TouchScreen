006202  000394  func ledDigitsDisplay(var imgidx, var typeptr, var setptr)
006203  000395      var i, j, k, l, lb, newval ;
006204  000396      if (!((img_GetWord(hndl, imgidx, IMAGE_FLAGS) & I_ENABLED))) return ;  // ;img_GetWord(hndl, imgidx, IMAGE_TAG2) ;if diabled then exit
006205  000397      newval := img_GetWord(hndl, imgidx, IMAGE_TAG2) ;
006206  000398      i := -1 ;
006207  000399      j := *(typeptr) ;
006208  000400      repeat
006209  000401          typeptr += 2 ;
006210  000402          i++ ;
006211  000403          until (*(typeptr) == imgidx);
006212  000404      j := setptr + i*10 ;
006213  000405        
006214  000406      imgidx++ ;
006215  000407      l := 1 ;
006216  000408      for (i := 1; i < *(j+iDigits); i++)
006217  000409          l *= 10 ;
006218  000410      next
006219  000411      lb := *(j+iLeadingBlanks) ;
006220  000412      for (i := 0; i < *(j+iDigits); i++)
006221  000413          k := newval / l ;
006222  000414          newval -= k * l ;
006223  000415          if ( lb && (i < *(j+iDigits) - *(j+iMinDigits)) )
006224  000416              if (k == 0)
006225  000417                  k := 10 ;
006226  000418              else
006227  000419                  lb := 0 ;
006228  000420              endif
006229  000421          endif
006230  000422          l /= 10 ;
006231  000423          img_SetWord(hndl, imgidx, IMAGE_INDEX, k);
006232  000424          img_SetWord(hndl, imgidx, IMAGE_XPOS, *(j+ileft)+i* *(j+iWidthDigit)) ;
006233  000425          img_Show(hndl, imgidx);
006234  000426      next
006235  000427  endfunc
