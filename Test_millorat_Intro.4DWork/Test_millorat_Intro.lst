006280  000472  func UpdateObjects(var newval)
006281  000473      var IPidx, otherOBJ ;
006282  000474      if ( ( img_GetWord(hndl, *(object), IMAGE_INDEX) != newval) || (TState == IPD_RELEASE) ) // only bother if values changed, or release
006283  000475        
006284  000476          img_SetWord(hndl, *(object), IMAGE_INDEX, newval);
006285  000477  // temp for **User??       if ((inputtype != tUserButton) || (!*(input+IPD_P1)))
006286  000478              img_Show(hndl, *(object));      // only shows on current form
006287  000479  //        endif
006288  000480          if ((inputtype == t4Dbutton) || (inputtype == tUserButton) || (inputtype == tWinButton)) 
006289  000481              if (*(input+IPD_P1))
006290  000482                  newval &= 1;
006291  000483              else
006292  000484                  newval &= 3;
006293  000485              endif
006294  000486              if (newval > 1) newval := 1;
006295  000487          endif
006296  000488          IPidx := *(input+TState) ;
006297  000489          while(IPidx != 0)
006298  000490              otherOBJ := IPidx + IPData;
006299  000491              if (*(otherOBJ) == OT_REPORT)
006300  000492                  seroutCS(REPORT_EVENT) ;
006301  000493                  seroutCS(inputtype) ;
006302  000494                  seroutCS(*(otherOBJ+IPD_OBJVidx)) ;
006303  000495                  seroutCS(newval >> 8) ;
006304  000496                  seroutCS(newval) ;
006305  000497                  seroutOcs() ;
006306  000498              else if (TState == *(otherOBJ+IPD_P4))
006307  000499                  if (*(otherOBJ) == OT_ACTIVATE)
006308  000500                      ActivateForm(*(otherOBJ+IPD_P2) ) ;
006309  000501                      inputtype := tForm ;
006310  000502                  else if (*(otherOBJ) == OT_SETCONST)
006311  000503                      newval := *(otherOBJ+IPD_P3) ;
006312  000504                      WriteObject(*(otherOBJ+IPD_P1), *(otherOBJ+IPD_P2), newval) ;
006313  000505                  else if (*(otherOBJ) == OT_SETANOTHER)
006314  000506                      WriteObject(*(otherOBJ+IPD_P1), *(otherOBJ+IPD_P2), newval) ;
006315  000507                  else if (*(otherOBJ) == OT_PREVFRAME)
006316  000508                      if (img_GetWord(hndl, *(otherOBJ+IPD_P6), IMAGE_INDEX))
006317  000509                          WriteObject(*(otherOBJ+IPD_P5),*(otherOBJ+IPD_P2),img_GetWord(hndl, *(otherOBJ+IPD_P6), IMAGE_INDEX)-1) ;
006318  000510                      endif
006319  000511                      newval := img_GetWord(hndl, *(otherOBJ+IPD_P6), IMAGE_INDEX) ;
006320  000512                  else if (*(otherOBJ) == OT_NEXTFRAME)
006321  000513                      if (img_GetWord(hndl, *(otherOBJ+IPD_P6), IMAGE_INDEX) < *(otherOBJ+IPD_P3)) 
006322  000514                          WriteObject(*(otherOBJ+IPD_P5),*(otherOBJ+IPD_P2),img_GetWord(hndl, *(otherOBJ+IPD_P6), IMAGE_INDEX)+1) ;
006323  000515                      endif
006324  000516                      newval := img_GetWord(hndl, *(otherOBJ+IPD_P6), IMAGE_INDEX) ;
006325  000517                  else if (*(otherOBJ) == OT_PREVSTRING)
006326  000518                      if (stringsCV[*(otherOBJ+IPD_P2)])
006327  000519                          WriteObject(tStrings,*(otherOBJ+IPD_P2),stringsCV[*(otherOBJ+IPD_P2)]-1) ;
006328  000520                      endif
006329  000521                  else if (*(otherOBJ) == OT_NEXTSTRING)
006330  000522                      if (stringsCV[*(otherOBJ+IPD_P2)] < *(otherOBJ+IPD_P3)) // fix IPD_P2 not filled in yet
006331  000523                          WriteObject(tStrings,*(otherOBJ+IPD_P2),stringsCV[*(otherOBJ+IPD_P2)]+1) ;
006332  000524                      endif
006333  000525                  endif
006334  000526              endif
006335  000527              IPidx := *(otherOBJ+TState) ;
006336  000528          wend
006337  000529      endif
006338  000530  endfunc
