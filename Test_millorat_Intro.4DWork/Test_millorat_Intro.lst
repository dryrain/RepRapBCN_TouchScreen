006238  000430  func ActivateForm(var newform)
006239  000431      var i, j, *p ;
006240  000432      if (curform != -1) // deactivate old form, by disabling all inputs
006241  000433          for (i := formstidx[curform]; i <= formenidx[curform]; i++)
006242  000434              if (img_GetWord(hndl, i, IMAGE_TAG))
006243  000435                  img_Disable(hndl,i) ;
006244  000436              endif
006245  000437          next
006246  000438      endif
006247  000439      curform := newform ;
006248  000440      // display newform image or clear to image color
006249  000441      if (formBGcol[curform] != BGIMAGE)
006250  000442          gfx_Set(BACKGROUND_COLOUR,formBGcol[curform]);
006251  000443          gfx_Cls() ;
006252  000444          DoGFXObjects() ;                                    // display GFX 'widgets'
006253  000445      endif
006254  000446        
006255  000447      // enable inputs
006256  000448      for (i := formstidx[curform]; i < formenidx[curform]; i++)
006257  000449          j := img_GetWord(hndl, i, IMAGE_TAG) ;
006258  000450          if (j)
006259  000451              j-- ;
006260  000452              img_SetAttributes(hndl, i, I_STAYONTOP+I_ENABLED);        // make sure this is on top of form, if applicable
006261  000453              //if (j != tKeyboard)
006262  000454              if ((j <= tWinButton) || (j >= t4Dbutton) )               // enable inputs
006263  000455                  img_ClearAttributes(hndl, i, I_TOUCH_DISABLE);        // ensure touch is enabled
006264  000456              endif
006265  000457              img_Show(hndl,i) ; // show initialy, if required
006266  000458              if (j == tForm)
006267  000459                  DoGFXObjects() ;                                    // display GFX 'widgets' for image backgruobds
006268  000460              else if (j == tLeddigits)
006269  000461                  ledDigitsDisplay(i, oLeddigitss, OPDataL) ;
006270  000462              endif
006271  000463          endif
006272  000464      next
006273  000465      for (i := 0; i < oStringss[0]; i++)
006274  000466          if (stringsCV[i] != -1)
006275  000467              WriteObject(tStrings, i, stringsCV[i]) ;
006276  000468          endif
006277  000469      next
006278  000470  endfunc
